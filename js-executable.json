{
  "extends": [
    ":automergeMinor"
  ],
  "rangeStrategy": "auto",
  "travis": {
    "enabled": true
  },
  "node": {
    "supportPolicy": "current"
  },
  "packageRules": [
    {
      "packagePatterns": [
        "^eslint-config-",
        "/eslint-plugin$"
      ],
      "groupName": "eslint packages",
      "separateMajorMinor": false
    }
  ]
}
